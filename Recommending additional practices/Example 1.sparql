# The agile practice "Scrum of Scrums can" be recommended if any results
# are obtained from this query

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX ag:   <http://www.semanticweb.org/utn/AgileOntology#>

SELECT ?profile ?value 
WHERE {
?profile rdf:type ag:TargetAgileProfile ;
                  ag:hasTeam ?team .
?team   ag:hasTeamCharacteristic ?teamch .
?teamch rdf:type ag:TeamSize ;
                 ag:hasValue ?value .
Filter (?value=<http://www.semanticweb.org/utn/AgileOntology#10+(toolarge)>)
}
